{% extends '/admin/admin_base.html.twig' %}

{% block title %}Ajouter un trait{% endblock %}

{% block body %}
    <div class="container">
        <h1>Ajouter un trait</h1>
        {{  form_start(form) }}

        <div class="form-row">
            {{  form_row(form.nom) }}
        </div>

        <div class="form-row">
            {{  form_row(form.description) }}
        </div>

        <div class="form-row">
            {{  form_row(form.placeMoyenne) }}
        </div>

        {{  form_end(form) }}
    </div>


{% endblock %}
