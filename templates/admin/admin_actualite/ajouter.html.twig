{% extends '/admin/admin_base.html.twig' %}

{% block title %}Ajouter une actualité{% endblock %}

{% block body %}
    <div class="container">
        <h1>Ajouter une actualité</h1>
        {{  form_start(form) }}

        <div class="form-row">
            {{  form_row(form.titre) }}
        </div>

        <div class="form-row">
            {{  form_row(form.texte) }}
        </div>

        <div class="form-row">
            {{  form_row(form.illustration) }}
        </div>

        {{  form_end(form) }}
    </div>


{% endblock %}
