{% extends '/admin/admin_base.html.twig' %}

{% block title %}Ajouter une composition{% endblock %}

{% block body %}
    <div class="container">
        <h1>Ajouter une composition</h1>
        {{  form_start(form) }}

        <div class="form-row">
            {{  form_row(form.nom) }}
        </div>

        <div class="form-row">
            {{  form_row(form.classement) }}
        </div>

        <div class="form-row">
            {{  form_row(form.champions) }}
        </div>

        {{  form_end(form) }}
    </div>


{% endblock %}
